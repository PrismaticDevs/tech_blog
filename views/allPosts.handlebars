<ul class="list-group mt-5">
  {{#each posts as |post|}}
    <li class="list-group-item">
      <section class="d-flex">
        <h1>{{post.title}}</h1>
        <p class="p-3">{{post.id}}</p>
      </section>
      <article>
        <p>{{post.body}}</p>
        <p>Created By: {{post.userId}}</p>
      </article>

    </li>
  {{/each}}
</ul>