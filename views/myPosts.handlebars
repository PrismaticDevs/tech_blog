<section class="mt-5">
  <br>
  {{createPost}}
</section>
<ul class="list-group">
  {{#each posts as |post|}}
  <h1>{{post.user.username}}'s' Posts</h1>
    <li class="list-group-item">
      <article class="post-article">
        <h2>{{post.title}}</h2>
        <article>{{post.body}}</article>
        <a href="/posts/{{post.id}}">View post</a>
        <p>Created By: {{post.user.username}}</p>
      </article>
    </li>
  {{/each}}
</ul>
</div>

